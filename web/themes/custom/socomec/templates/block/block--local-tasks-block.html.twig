{# extends @bootstrap_barrio/layout/block.html.twig
/**
 * @file
 * Theme override for tabs.
 */
#}
{% block content %}
  {% if content %}
    <nav class="tabs-local-tasks tabs navbar navbar-expand-xl navbar-light" role="navigation" aria-label="{{ 'Tabs'|t }}">
      <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#tabs-local-tasks-block" aria-controls="tabs-local-tasks-block" aria-expanded="false" aria-label="Toggle navigation">
        <span class="navbar-toggler-icon"></span>
      </button>
      <div class="collapse navbar-collapse" id="tabs-local-tasks-block">
        {{ content }}
      </div>
    </nav>
  {% endif %}
{% endblock %}
