{#
/**
 * @file
 * Display Suite Socomec bootstrap two columns 6/6.
 *
 * Available variables:
 * - outer_wrapper: outer wrapper element
 * - first_wrapper: wrapper element around first region
 * - second_wrapper: wrapper element around second region
 * - attributes: layout attributes
 * - first_attributes: attributes for first region
 * - second_attributes: attributes for second region
 * - first: content of first region
 * - second: content of second region
 */
#}

<{{ outer_wrapper }}{{ attributes.addClass('row', 'clearfix', 'custom-socomec-layout' , 'custom-socomec-layout-6-6') }}>

  {{ title_suffix.contextual_links }}

  <{{ first_wrapper }}{{ first_attributes.addClass('col-lg-6', 'custom-socomec-layout-left') }}>
    {{ first }}
  </{{ first_wrapper }}>

  <{{ second_wrapper }}{{ second_attributes.addClass('col-lg-6', 'custom-socomec-layout-right') }}>
    {{ second }}
  </{{ second_wrapper }}>

</{{ outer_wrapper }}>

