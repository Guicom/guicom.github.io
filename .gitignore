# Ignore directories generated by Composer
/drush/contrib/
/vendor/
/web/core/
/web/modules/contrib/
/web/themes/contrib/
/web/profiles/contrib/
/web/libraries/
/web/node_modules
/config/drupal/local

# Ignore local settings file
/config/drupal/keys/*
/web/sites/*/settings.php
/web/sites/*/settings.local.php
/web/sites/*/services.yml
/phing/build.properties.local
/config/drupal/settings.local.php
/config/drupal/*/settings.local.php
/config/drupal/*/services.local.yml
!/config/drupal/template/settings.local.php
!/config/drupal/template/services.local.yml

# Ignore overrides config
/config/drupal/overrides
/config/drupal/pim_configuration/*

# Ignore sensitive information
/web/sites/**/files/*
/data/**/files/private
/data/db
!/data/db/tag/
!/web/sites/**/files/label/

# Ignore sites.php
/web/sites/sites.php

# Ignore SimpleTest multi-site environment.
/web/sites/simpletest

# Ignore files generated by PhpStorm
.idea
**/.DS_Store

# Ignore .env files as they are personal
/.env

# Local docker compose file
/.docker-sync/
docker-compose.yml
docker-compose.override.yml
docker-sync.yml

# Ignore compiled files
/web/themes/custom/*/assets/css/*.css

# Ignore SQL files
*.sql
/data/characteristics_dictionary**.json
