{
    "name": "actency/socomec",
    "description": "Drupal 8 multisite",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "authors": [
        {
            "name": "",
            "role": ""
        }
    ],
    "repositories": {
        "0": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        },
        "actency/foundation": {
            "type": "vcs",
            "url": "https://gitlab.com/actency/utils/drupal/foundation.git"
        },
        "drupal/phingdrushtask": {
            "type": "vcs",
            "url": "https://git.drupal.org/project/phingdrushtask.git"
        }
    },
    "require": {
        "php": ">=7.2",
        "ext-SimpleXML": "^7.1",
        "ext-curl": "^7.1",
        "ext-json": "^1.5",
        "actency/foundation": "^8.1",
        "composer/installers": "^1.7",
        "cweagans/composer-patches": "^1.6.5",
        "drupal-composer/drupal-scaffold": "^2.5",
        "drupal/abjs": "^1.2",
        "drupal/address": "^1.7",
        "drupal/address_map_link": "^1.0",
        "drupal/addtoany": "^1.12",
        "drupal/addtocalendar": "^3.1",
        "drupal/admin_toolbar": "^1.27",
        "drupal/allow_iframed_site": "^2.0",
        "drupal/auto_entitylabel": "^3.0@beta",
        "drupal/better_exposed_filters": "^4.0@alpha",
        "drupal/better_normalizers": "^1.0@beta",
        "drupal/block_content_permissions": "^1.8",
        "drupal/bootstrap_sass": "^1.9",
        "drupal/conditional_fields": "^1.0@alpha",
        "drupal/config_ignore": "^2.1",
        "drupal/config_split": "^1.4",
        "drupal/console": "^1.0.2",
        "drupal/default_content": "1.x-dev",
        "drupal/default_content_deploy": "^1.0",
        "drupal/dropdown_language": "^2.3",
        "drupal/drupal-extension": "^4.0",
        "drupal/drush_language": "^1.0@RC",
        "drupal/ds": "^3.3",
        "drupal/editor_advanced_link": "^1.4",
        "drupal/entity_share": "^2.0@alpha",
        "drupal/entity_share_cron": "^2.0@alpha",
        "drupal/entity_usage": "^2.0@beta",
        "drupal/eu_cookie_compliance": "^1.6",
        "drupal/extra_field": "^1.1",
        "drupal/facets": "^1.4",
        "drupal/facets_pretty_paths": "^1.0",
        "drupal/field_group": "^1.0",
        "drupal/fontawesome": "^2.14",
        "drupal/gdpr": "^2.0@alpha",
        "drupal/google_analytics": "^3.0",
        "drupal/hook_event_dispatcher": "^1.25",
        "drupal/hotjar": "^1.5",
        "drupal/image_effects": "^2.0",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/international_phone": "^1.0",
        "drupal/libraries": "^3.0@alpha",
        "drupal/link_target": "^1.2",
        "drupal/menu_link_attributes": "^1.0",
        "drupal/menu_per_role": "^1.1",
        "drupal/metatag": "^1.8",
        "drupal/multiselect": "^1.0",
        "drupal/paragraphs": "^1.8",
        "drupal/pathauto": "^1.6",
        "drupal/recaptcha": "^2.4",
        "drupal/redirect": "^1.3",
        "drupal/redis": "^1.2",
        "drupal/remote_stream_wrapper": "^1.3",
        "drupal/remote_stream_wrapper_widget": "^1.2",
        "drupal/scheduled_updates": "^1.0@alpha",
        "drupal/scheduler": "^1.1",
        "drupal/scheduler_content_moderation_integration": "^1.1",
        "drupal/search_api": "^1.14",
        "drupal/search_api_autocomplete": "^1.2",
        "drupal/search_api_solr": "^3.7",
        "drupal/smart_trim": "^1.2",
        "drupal/svg_image": "^1.9",
        "drupal/taxonomy_menu": "^3.4",
        "drupal/twig_tweak": "^2.4",
        "drupal/ultimate_cron": "^2.0@alpha",
        "drupal/we_megamenu": "^1.7",
        "drupal/we_megamenu_deploy": "^1.0@alpha",
        "drupal/youtube": "^1.0",
        "drush/drush": "^9.0.0",
        "guzzlehttp/guzzle": "^6.3",
        "phpoffice/phpspreadsheet": "^1.10",
        "predis/predis": "^1.1",
        "symfony/event-dispatcher": "4.3.4 as 3.4.99",
        "tecnickcom/tcpdf": "^6.3",
        "vlucas/phpdotenv": "^2.4",
        "webflo/drupal-finder": "^1.0.0",
        "webmozart/path-util": "^2.3",
        "zaporylie/composer-drupal-optimizations": "^1.0"
    },
    "require-dev": {
        "drupal/devel": "^2.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ],
        "files": [
            "load.environment.php"
        ]
    },
    "scripts": {
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "composer-exit-on-patch-failure": true,
        "patchLevel": {
            "drupal/core": "-p2"
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/{$name}": [
                "type:drupal-drush"
            ]
        },
        "drupal-scaffold": {
            "initial": {
                ".editorconfig": "../.editorconfig",
                ".gitattributes": "../.gitattributes"
            }
        },
        "merge-plugin": {
            "include": [
                "vendor/actency/foundation/composer.json"
            ]
        },
        "patches": {
            "drupal/default_content_deploy": {
                "Add folder argument on drush dcdi dcde": "https://www.drupal.org/files/issues/2019-06-14/3061417-9.patch",
                "Fix NULL id n import":"./patches/default_content_deploy_null_id.patch"
            },
            "drupal/metatag": {
                "Add hreflang to Metatag": "https://www.drupal.org/files/issues/2018-09-06/2956982-6.patch"
            },
            "drupal/google_analytics": {
                "Google analytics eu_cookie_compliance support for 8.x-3.x": "https://www.drupal.org/files/issues/2020-01-14/google_analytics-eu_cookie_compliance_support-2917905-49.patch"
            },
            "drupal/facets": {
                "Facets Show reset link of the LinksWidget do not work when facets_pretty_path is active to 8.x-1.x": "https://www.drupal.org/files/issues/2020-01-29/show_reset_link_when_pretty_path_is_active-3079451-3.patch"
            },
            "drupal/we_megamenu": {
                "Add taxonomy menu compatibnilty": "./patches/we_mega_menu_menu_taxonmy_integration.patch"
            },
            "drupal/ultimate_cron": {
                "port drush commands to drush 9.x": "https://www.drupal.org/files/issues/2020-03-21/2912724-21.patch"
            }
        }
    }
}
